{
  "name": "holz",
  "version": "1.0.1",
  "description": "Holz: A fast, minimal TypeScript logger",
  "main": "dist/index.js",
  "scripts": {
    "bench": "npm run build && node benchmark",
    "prepare": "npm run build",
    "build": "rimraf 'dist' && tsc -b",
